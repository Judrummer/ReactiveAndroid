<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.github.kittinunf.reactiveandroid.sample"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.LandingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".view.SignInActivity">
        </activity>
        <activity android:name=".view.SignUpActivity">
        </activity>
        <activity android:name=".view.RecyclerViewActivity">
        </activity>
        <activity
            android:name=".view.FragmentPagerActivity"
            android:label="@string/title_activity_fragment_pager"
            android:theme="@style/AppTheme">
        </activity>
    </application>

</manifest>